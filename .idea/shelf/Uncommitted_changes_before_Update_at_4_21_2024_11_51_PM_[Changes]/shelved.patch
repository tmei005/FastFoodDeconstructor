>\r\n<!--        <form id=\"search-form\" action=\"/process_input2\" method=\"POST\">-->\r\n            <button class=\"page2-button\"><i class=\"fa-solid fa-magnifying-glass\"></i></button>\r\n<!--        </form>-->\r\n        <script>\r\n    // Add an event listener to the magnifying glass button\r\n    document.getElementById(\"magnifying-glass\").addEventListener('click', function(event) {\r\n        event.preventDefault(); // Prevent the default form submission\r\n\r\n        // Get the selected category\r\n        const selectedCategory = document.querySelector('.selected').innerText;\r\n\r\n        // Retrieve all items in the selected category\r\n        const itemsInCategory = restaurant_cat_map.filter(pair => pair[0] === selectedCategory)[0][1];\r\n\r\n        // Display the items (for demonstration purposes, you can adjust this part)\r\n        alert(`Items in ${selectedCategory}: ${itemsInCategory.join(', ')}`);\r\n    });\r\n</script>\r\n    </div>\r\n    <script>\r\n        // get all dropdowns\r\n        const dropdowns = document.querySelectorAll('.dropdown');\r\n        // loop through all dropdown elements\r\n        dropdowns.forEach(dropdown => {\r\n            // get inner elements from each dropdonw\r\n            const select = dropdown.querySelector('.select');\r\n            const caret = dropdown.querySelector('.caret');\r\n            const menu = dropdown.querySelector('.menu');\r\n            const options = dropdown.querySelectorAll('.menu li');\r\n            const selected = dropdown.querySelector('.selected');\r\n\r\n            // add a click event to the select element\r\n            select.addEventListener('click', () => {\r\n                // add the clicked select styles to the select element\r\n                select.classList.toggle('select-clicked')\r\n                // add the rotate styles to the caret element\r\n                caret.classList.toggle('caret-rotate');\r\n                // add the open styles to the menu elements\r\n                menu.classList.toggle('menu-open');\r\n            });\r\n\r\n            // loop through all option elements\r\n            options.forEach(option => {\r\n                // add a click event to the option element\r\n                option.addEventListener('click', () => {\r\n                    // change selected inner text to clicked option inner text\r\n                    selected.innerText = option.innerText;\r\n                    // add the clicked select styles to the select element\r\n                    select.classList.remove('select-clicked');\r\n                    // add the rotate styles to the caret element\r\n                    caret.classList.remove('caret-rotate');\r\n                    // add the open styles to the menu element\r\n                    menu.classList.remove('menu-open');\r\n                    // remove active class from all option elements\r\n                    options.forEach(option => {\r\n                        option.classList.remove('active');\r\n                    });\r\n                    // add active class to clicked option element\r\n                    option.classList.add('active');\r\n                    // Inside the event listener for sorting selection\r\n                    const selectedSortingInput = document.getElementById(\"selectedSorting\");\r\n                    selectedSortingInput.value = selected.innerText; // Update the value of the hidden input field\r\n                });\r\n            });\r\n        });\r\n        // Inside the event listener for sorting selection\r\n        const sortingOptions = document.querySelectorAll('.menu.sort li');\r\n        sortingOptions.forEach(option => {\r\n            option.addEventListener('click', () => {\r\n                // Update the value of the hidden input field for sorting\r\n                const selectedSortingInput = document.getElementById(\"selectedSorting\");\r\n                selectedSortingInput.value = option.innerText;\r\n            });\r\n        });\r\n\r\n    </script>\r\n    <a href=\"page3.html\">click me!</a>\r\n    <div id=\"cat\"></div>\r\n    <img id=\"catAnimation\" src=\"../static/Images/cat1.png\" alt=\"cat burger\" class=\"cat-image\">\r\n    <script src=\"{{ url_for('static', filename='cat.js') }}\"></script>\r\n</body>\r\n</html>
